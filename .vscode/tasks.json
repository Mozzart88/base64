{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build tests",
            "command": "/usr/bin/clang",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
				"-I",
				"${workspaceFolder}/include/",
                "${workspaceFolder}/test/test.c",
                "${workspaceFolder}/src/decode.c",
                "${workspaceFolder}/src/encode.c",
                "${workspaceFolder}/test/decode.test.c",
                "${workspaceFolder}/test/encode.test.c",
                "-o",
                "${workspaceFolder}/sandbox/test"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}